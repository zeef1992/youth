	color: #FF85CB
}
.font_weight{
	font-weight: bold;
}

/**************************************************
					Width
**************************************************/
.width170pc {
	width: 170%!important;
}
.overflow-x {
	overflow-x: auto!important;
}
.margin_bottom5 {
	margin-bottom: 5px;
}