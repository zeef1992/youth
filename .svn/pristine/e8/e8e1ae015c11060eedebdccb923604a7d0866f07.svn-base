	margin: 0 auto;
}
#tblBody  tr:first-child  td {
	border-top: none !important;
}
#divBody{
	overflow-x: hidden;
	/*height: 400px;
	border-bottom: 1px solid #808080;*/
}

/*	 ========================================================================== */
/* スマートフォン用 
  ============================================================================= */
/* Tablet display */
@media only screen and (min-width: 600px) and (max-width: 768px){
	.div-content {
		margin: 135px 5px;
		min-width: 600px;
	}
	
	.div-content-lv2 {
		width: 97%;
	}
	
	.text-title-lv2 {
		float: left;
		width: 45%;
	}
	.text-title-lv3 {
		float: left;
		width: 30%;
	}
	.text-title-lv4 {
		float: left;
		width: 20%;
	}
	.tbl-row {
		display: block;
		width: 100%;
	}
	.tbl-left {
		float: none;
		text-align: left;
		width: 100%;
		margin-bottom: 0px;
	}
	.login-box .tbl-left {
		width: 100%;
	}
		.tbl-left span.tbl-left-text {
			margin-right: 0;
		}

	.tbl-right {
		float: none;
		width: 100%;
		margin-bottom: 10px;
	}
	.login-box .tbl-right {
		width: 100%;
	}
	
	.tbl-line-checkbox {
		float: left;
		width: 100%;
	}
	
	.tbl-checkbox {
		float: left;
		width: 45%;
	}
	
	.tbl-line-radio {
		float: left;
		width: 100%;
	}
	
	.tbl-radio {
		float: left;
		width: 45%;
	}
	.cbx-lv4 {
		width: 98%;
	}
	.txt-lv2,.txt-lv3,.txt-lv4,.txtarea-lvl {
		width: 97%;
	}
	.txt-lvt {
		width: 80%;
	}
	.txt-lvt-btn {
		width: 70%;
	}

	.cont-box {
		width: 100%;
		margin: 0 auto;
	}
	.cont-pop-box {
		width: 100%;
		margin: 0 auto;
	}
	.tbl-select-area {
		width: 100%;
	}
	
	.group-button-lv4 {
		width: 101%;
		padding-left: 5px;
		padding-right: 5px;
		margin: 0 0 0 0;
	}
	
	.btn-medium-lv2 {
		width: 24%;
		height: 35px;
		background: silver;
		border: 1px solid #dcdcde;
		color: #fff;
		font-weight: bold;
		cursor: pointer;
	}
	
	/* Popup display */
	
	.popup-title {
		width: calc(100% - 40px);
		width: -webkit-calc(100% - 40px);
		max-width: 768px;
		min-width: 600px;
	}
	#popupSearchUser {
		width: calc(100% - 40px) !important;
		height: calc(100% - 170px) !important;
	}
	.div-pop-content {
		margin: 30px 5px;
		min-width: 600px;
	}

	#popupFarmEdit {
		width: calc(100% - 40px) !important;
		height: calc(100% - 170px) !important;
	}
	
	#popupCultivation {
		width: calc(100% - 40px) !important;
		height: calc(100% - 170px) !important;
	}
	
	#popupBlockEditUser {
		width: calc(100% - 40px) !important;
		height: calc(100% - 170px) !important;
	}
	
	#popupEditArea{
		width: calc(100% - 40px) !important;
		height: calc(100% - 170px) !important;
	}
	
	.popup-content .div-content {
		margin: 30px 5px;
		min-width: 600px;
	}

	.popup-content .div-content-lv2 {
		margin: 30px 5px;
		width: 100%;
		min-width: 600px;
	}
	
	.body-scroll {
		overflow: hidden !important;
	}
	
	.popup-scroll {
		overflow: scroll !important;
	}
	.height {
		height: 295px;
	}
}

