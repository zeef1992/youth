.drop-menu ul li {
	float: right;
	font-size: 14px;
	font-weight: bold;
	height: 30px;
	line-height: 30px;
	list-style: outside none none;
	margin: 0;
	padding: 0;
	position: relative;
	text-align: center;
}
	.drop-menu ul li a {
		color: #4d4d4d;
		display: block;
		text-decoration: none;
	}
	.drop-menu ul li a {
		padding-left: 5px;
	}
	.drop-menu ul li:hover, .drop-menu ul li a:hover {
		background: #198B0D none repeat scroll 0 0;
		color: #fff;
	}
	.drop-menu ul li ul {
		position: absolute;
		top: 30px;
		z-index: 100;
	}
		.drop-menu ul li ul li {
			background: #C1E1B9 none repeat scroll 0 0;
			border-top: 1px solid #198B0D;
			height: 0;
			overflow: hidden;
			visibility: hidden;
			width: 135px;
		}
		.drop-menu ul li ul li:hover, .drop-menu ul li ul li a:hover {
			background: #194D07 none repeat scroll 0 0;
		}

	.drop-menu ul li:hover ul li, .drop-menu ul li a:hover ul li {
		height: 30px;
		overflow: visible;
		visibility: visible;
		z-index: 10;
	}

.drop-menu * {
	transition: all 0.5s ease 0s;
}

